{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "gdb",
            "request": "attach",
            "name": "Attach to gdbserver",
            "gdbpath": "arm-none-eabi-gdb",
            "executable": "./target/thumbv7em-none-eabihf/debug/registers",
            "target": ":3333",
            "remote": true,
            "cwd": "${workspaceRoot}",
            "autorun": [
                "load",
                "monitor tpiu config internal tmp/itm.log uart off 8000000",
                "break main",
                "continue"
            ]
        }
    ]
}